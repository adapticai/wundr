{
  "name": "@wundr.io/cli",
  "version": "1.0.0",
  "description": "Unified CLI framework for the Wundr platform",
  "keywords": ["wundr", "cli", "commander", "interactive", "tui", "automation"],
  "homepage": "https://wundr.io",
  "bugs": {
    "url": "https://github.com/adapticai/wundr/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adapticai/wundr.git"
  },
  "license": "MIT",
  "author": "Wundr, by Adaptic.ai",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "wundr": "./bin/wundr.js"
  },
  "files": ["dist", "bin", "src", "templates", "README.md"],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "ts-node src/index.ts",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@oclif/core": "^3.15.0",
    "@oclif/plugin-help": "^6.0.0",
    "@oclif/plugin-plugins": "^4.1.0",
    "@types/axios": "^0.9.36",
    "@wundr.io/computer-setup": "workspace:*",
    "@wundr.io/core": "workspace:*",
    "@wundr.io/config": "workspace:*",
    "axios": "^1.11.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "conf": "^12.0.0",
    "execa": "^8.0.1",
    "figlet": "^1.7.0",
    "fs-extra": "^11.3.1",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12",
    "listr2": "^8.0.0",
    "node-pty": "^1.0.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "rxjs": "^7.8.1",
    "terminal-kit": "^3.0.1",
    "ws": "^8.16.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/prompts": "^2.4.9",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "oclif": {
    "bin": "wundr",
    "dirname": "wundr",
    "commands": "./dist/commands",
    "plugins": ["@oclif/plugin-help", "@oclif/plugin-plugins"],
    "topicSeparator": ":",
    "topics": {
      "init": {
        "description": "Initialize Wundr projects and configurations"
      },
      "create": {
        "description": "Create new components, services, and templates"
      },
      "analyze": {
        "description": "Code analysis and dependency management"
      },
      "govern": {
        "description": "Governance and compliance tools"
      },
      "ai": {
        "description": "AI-powered development features"
      },
      "dashboard": {
        "description": "Dashboard and visualization tools"
      },
      "setup": {
        "description": "Computer setup and provisioning for developer machines"
      }
    }
  }
}
