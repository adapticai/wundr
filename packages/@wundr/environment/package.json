{
  "name": "@wundr/environment",
  "version": "1.0.0",
  "description": "Cross-platform development environment setup and management tools",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "wundr-env": "bin/wundr-env.js",
    "wundr-quickstart": "bin/quickstart.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "install:macos": "./scripts/install/macos.sh",
    "install:linux": "./scripts/install/linux.sh",
    "install:windows": "./scripts/install/windows.ps1",
    "setup:human": "./scripts/setup-profile.sh human",
    "setup:ai-agent": "./scripts/setup-profile.sh ai-agent",
    "setup:ci-runner": "./scripts/setup-profile.sh ci-runner",
    "quickstart": "./bin/quickstart.js",
    "quickstart:minimal": "./bin/quickstart.js --preset=minimal",
    "quickstart:standard": "./bin/quickstart.js --preset=standard",
    "quickstart:full": "./bin/quickstart.js --preset=full",
    "quickstart:parallel": "./scripts/install/quickstart-parallel.sh",
    "benchmark": "jest tests/benchmark/quickstart-benchmark.test.ts --testTimeout=600000",
    "validate": "./scripts/validation/validate-environment.sh",
    "health-check": "./scripts/validation/health-check.sh"
  },
  "keywords": [
    "development-environment",
    "setup",
    "quickstart",
    "fast-setup",
    "parallel-installation",
    "cross-platform",
    "node.js",
    "docker",
    "vscode",
    "ai-agents",
    "claude-code",
    "mcp-tools",
    "performance",
    "automation"
  ],
  "dependencies": {
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "ora": "^7.0.0",
    "fs-extra": "^11.1.0",
    "semver": "^7.5.0",
    "which": "^4.0.0",
    "execa": "^8.0.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/semver": "^7.5.0",
    "@types/which": "^3.0.0",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "typescript": "^5.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.50.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wundr/wundr.git",
    "directory": "packages/@wundr/environment"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "restricted"
  }
}