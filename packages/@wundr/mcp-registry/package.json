{
  "name": "@wundr.io/mcp-registry",
  "version": "1.0.6",
  "description": "MCP Server registry and discovery with Super MCP aggregator pattern for unified tool routing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "uuid": "^11.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.2.2",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3"
  },
  "peerDependencies": {
    "@wundr.io/mcp-server": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@wundr.io/mcp-server": {
      "optional": true
    }
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "registry",
    "discovery",
    "aggregator",
    "super-mcp",
    "claude-code",
    "wundr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adapticai/wundr.git",
    "directory": "packages/@wundr/mcp-registry"
  },
  "bugs": {
    "url": "https://github.com/adapticai/wundr/issues"
  },
  "homepage": "https://wundr.io",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": ["dist", "README.md"],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
