{
  "name": "@wundr.io/structured-output",
  "version": "1.0.3",
  "description": "Pydantic/Instructor-style LLM output validation with retry loops and grammar enforcement",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.2.2",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "wundr",
    "structured-output",
    "llm",
    "validation",
    "zod",
    "instructor",
    "pydantic",
    "ai",
    "json-schema",
    "grammar-enforcement"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adapticai/wundr.git",
    "directory": "packages/@wundr/structured-output"
  },
  "bugs": {
    "url": "https://github.com/adapticai/wundr/issues"
  },
  "homepage": "https://wundr.io"
}
