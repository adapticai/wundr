  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "@types/validate-npm-package-name": "^4.0.2",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "wundr",
    "templates",
