{
  "name": "@wundr.io/project-templates",
  "version": "1.0.0",
  "description": "Wundr-compliant project templates with opinionated best practices",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "dev": "tsc --build --watch",
