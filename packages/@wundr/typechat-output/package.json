{
  "name": "@wundr.io/typechat-output",
  "version": "1.0.3",
  "description": "TypeScript-first validation using TypeChat patterns with schema-driven correction prompts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.25.76",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "wundr",
    "typechat",
    "typescript",
    "validation",
    "llm",
    "ai",
    "schema",
    "type-safe",
    "correction-prompts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adapticai/wundr.git",
    "directory": "packages/@wundr/typechat-output"
  },
  "bugs": {
    "url": "https://github.com/adapticai/wundr/issues"
  },
  "homepage": "https://wundr.io"
}
