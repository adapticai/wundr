{
  "name": "@wundr.io/langgraph-orchestrator",
  "version": "1.0.6",
  "description": "LangGraph-style cyclic, state-driven workflow orchestration for AI agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.25.76",
    "uuid": "^11.0.3",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.2.2",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "langgraph",
    "workflow",
    "orchestration",
    "state-machine",
    "cyclic-workflows",
    "ai-agents",
    "wundr"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adapticai/wundr.git",
    "directory": "packages/@wundr/langgraph-orchestrator"
  },
  "bugs": {
    "url": "https://github.com/adapticai/wundr/issues"
  },
  "homepage": "https://wundr.io"
}
