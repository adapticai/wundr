{
  "name": "@wundr.io/orchestrator-daemon",
  "version": "1.0.6",
  "description": "Orchestrator Daemon for agent orchestration and session management",
  "keywords": [
    "wundr",
    "orchestrator",
    "daemon",
    "agent-orchestration",
    "session-manager",
    "websocket",
    "claude-code",
    "claude-flow"
  ],
  "homepage": "https://wundr.io",
  "bugs": {
