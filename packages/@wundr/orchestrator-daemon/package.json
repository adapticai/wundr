      "types": "./dist/charter/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "orchestrator-daemon": "./bin/orchestrator-daemon.js"
  },
  "files": [
    "dist",
    "bin",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,md}'",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "npx tsx scripts/test-e2e.ts",
    "test:ws": "npx tsx scripts/test-websocket.ts",
    "typecheck": "tsc --noEmit",
    "start": "node bin/orchestrator-daemon.js",
    "start:dev": "./scripts/start-dev.sh",
    "start:docker": "./scripts/start-docker.sh",
    "health-check": "./scripts/health-check.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f daemon"
  },
  "dependencies": {
    "@adaptic/lumic-utils": "^1.0.6",
