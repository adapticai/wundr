{
  "name": "@wundr/genesis-app",
  "version": "0.1.0",
  "private": true,
  "description": "Genesis App - AI-powered organizational structure generation platform for Wundr hedge funds",
  "keywords": [
    "genesis",
    "ai",
    "organizational-structure",
    "hedge-fund",
    "wundr",
    "monorepo"
  ],
  "license": "UNLICENSED",
  "author": "Adaptic.ai Engineering Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/adaptic-ai/wundr.git",
    "directory": "packages/@wundr/genesis-app"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "pnpm --filter @genesis/* --parallel run dev",
    "build": "pnpm --filter @genesis/* run build",
    "lint": "pnpm --filter @genesis/* run lint",
    "lint:fix": "pnpm --filter @genesis/* run lint -- --fix",
    "typecheck": "pnpm --filter @genesis/* run typecheck",
    "test": "pnpm --filter @genesis/* run test",
    "test:watch": "pnpm --filter @genesis/* run test -- --watch",
    "test:coverage": "pnpm --filter @genesis/* run test -- --coverage",
    "clean": "pnpm --filter @genesis/* run clean && rm -rf node_modules .turbo",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "echo 'Genesis App monorepo ready'"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^20.14.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.0",
    "turbo": "^2.0.0",
    "typescript": "^5.4.5",
    "vitest": "^2.1.6",
    "@vitest/coverage-v8": "^2.1.6"
  }
}
